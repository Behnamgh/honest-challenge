{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "jsRules": {},
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "jsdoc-format": false,
    "object-literal-sort-keys":false,
    "quotemark": [true, "double"],
    "align":false,
    "array-type" :false
  },
  "rulesDirectory": []
}
